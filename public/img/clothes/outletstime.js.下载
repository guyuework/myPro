jQuery(document).ready(function(c){if(c("#temaiID").val()&&c("#request_is_outlets").val()==="1"){b()}function b(){var d="";c.ajax({url:"/ajax/detail/getoinfo/"+c("#temaiID").val()+"/",type:"get",dataType:"json",data:d,success:function(e){a(e);c(".time").each(function(){_this=c(this);var h={timediv:"#"+_this.attr("id"),useday:true};var g=new timecount(h);var f=new timefscount(h);g.init();f.init()})}})}function a(f){if(f.period=="soon"){var e='<span class="timetxt">距开始</span><div class="time" id="time1"></div>';c("#prodoutletstime").html(e).show();var d={nowtime:f.nowtime,endtime:f.starttime};c("#time1").data("time",d)}else{var e='<span class="timetxt">距结束</span><div class="time" id="time1"></div>';c("#prodoutletstime").html(e).show();var d={nowtime:f.nowtime,endtime:f.endtime};c("#time1").data("time",d)}}});