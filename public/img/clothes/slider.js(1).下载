jQuery(document).ready(function(e){function a(h){var g=document.cookie.match(new RegExp("(^| )"+h+"=([^;]*)(;|$)"));if(g!=null){return unescape(g[2])}return""}function f(l){var j="ABCDEFGHIJKLMNOPQRSTUVWXYZabcdefghijklmnopqrstuvwxyz0123456789+/=";var h="";var s,q,o="";var r,p,n,m="";var k=0;if(l.length%4!=0){return""}var g=/[^A-Za-z0-9\+\/\=]/g;if(g.exec(l)){return""}do{r=j.indexOf(l.charAt(k++));p=j.indexOf(l.charAt(k++));n=j.indexOf(l.charAt(k++));m=j.indexOf(l.charAt(k++));s=(r<<2)|(p>>4);q=((p&15)<<4)|(n>>2);o=((n&3)<<6)|m;h=h+String.fromCharCode(s);if(n!=64){h+=String.fromCharCode(q)}if(m!=64){h+=String.fromCharCode(o)}s=q=o="";r=p=n=m=""}while(k<l.length);return h}function c(){var h=0;var i=a("uidInfo");var j=f(i);var g=j.split("-");if(g.length>1){h=g[1]}return h}var b=c();window._BFD=window._BFD||{};_BFD.BFD_INFO={user_id:b,page_type:"account",client_id:"Chaolemai"};_BFD.bfdShow1=function(g){if(e("span").hasClass("sharToDiv")){showbind9(g)}else{showbind4(g)}var k=0;var j="";if(g){j+="<ul>";for(var h in g){k++;j+='<li><div class="img">';j+='<a href="'+g[h].url+'" target="_blank">';j+='<img src="'+g[h].simg+'" alt=""></a></div>';j+='<a href="'+g[h].url+'" target="_blank">';j+="<em>"+g[h].name+"</em>";j+="<em>"+g[h].subname+"</em></a>";j+="<p><span>ï¿¥ "+g[h].prc+"</span></p></li>";if(k>1&&k<15&&k%5===0){j+="</ul><ul>"}}j+="</ul>"}e(".productlist").html(j);d()};function d(){e(".productlist").Slider({prevId:".leftOn",nextId:".rightOn",shownum:5,offbtnleft:"left",offbtnright:"right",offline:1})}});