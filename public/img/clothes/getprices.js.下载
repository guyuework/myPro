jQuery(function(g){var c=g("[pid^=16],[pid^=17],[pid^=18],[pid^=19]");var f=c.length;var b=100;var a=Math.floor(f/b);for(var e=0;e<a;e++){h(e)}if(f%b!=0){d()}function h(i){var l=[];for(var j=0;j<b;j++){if((i*b+j)>=f){break}l.push(c.eq(i*b+j).attr("pid"))}var k=l.join("-");g.ajax({url:"/ajax/product/get_list_prices",context:document.body,type:"GET",data:{comids:k},dataType:"json",success:function(m){g.each(m.sale_pr,function(n){g('[pid="'+n+'"]').text(m.sale_pr[n]);g('[mid="'+n+'"]').text(m.market_pr[n]);var o=parseInt(m.sale_pr[n]/m.market_pr[n]*100)/10;g('[zid="'+n+'"]').text(o)})}})}function d(){var k=[];for(var i=0;i<f%b;i++){k.push(c.eq(a*b+i).attr("pid"))}var j=k.join("-");g.ajax({url:"/ajax/product/get_list_prices",context:document.body,type:"GET",data:{comids:j},dataType:"json",success:function(l){g.each(l.sale_pr,function(m){g('[pid="'+m+'"]').text(l.sale_pr[m]);g('[mid="'+m+'"]').text(l.market_pr[m]);var n=parseInt(l.sale_pr[m]/l.market_pr[m]*100)/10;g('[zid="'+m+'"]').text(n)})}})}});